<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" styleClass="background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreenController">
   <children>
      <Button id="logoutButton" contentDisplay="CENTER" layoutX="218.0" layoutY="324.0" mnemonicParsing="false" onAction="#handleLogout" styleClass="Button" stylesheets="@style.css" text="Logout" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button id="EditProfileButton" contentDisplay="CENTER" layoutX="206.0" layoutY="277.0" mnemonicParsing="false" onAction="#handleEditProfile" styleClass="Button" stylesheets="@style.css" text="Edit Profile" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="userProfile" layoutX="200.0" layoutY="231.0" mnemonicParsing="false" onAction="#handleUserProfile" styleClass="Button" stylesheets="@style.css" text="User Profile" textFill="WHITE" />
      <Button fx:id="submitReportButton" layoutX="192.0" layoutY="146.0" mnemonicParsing="false" onAction="#openSubmitReportPage" styleClass="Button" stylesheets="@style.css" text="Submit Report" textFill="WHITE" />
      <Button fx:id="viewReportsButton" layoutX="196.0" layoutY="188.0" mnemonicParsing="false" onAction="#handleViewSubmittedReports" styleClass="Button" textFill="WHITE" text="View Reports" />
   </children>
</AnchorPane>
