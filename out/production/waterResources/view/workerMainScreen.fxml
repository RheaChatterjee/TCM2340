<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" styleClass="background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreenController">
   <children>
      <Button id="logoutButton" contentDisplay="CENTER" layoutX="211.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleLogout" styleClass="Button" stylesheets="@style.css" text="Logout" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button id="EditProfileButton" contentDisplay="CENTER" layoutX="199.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleEditProfile" styleClass="Button" stylesheets="@style.css" text="Edit Profile" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="userProfile" layoutX="198.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleUserProfile" styleClass="Button" stylesheets="@style.css" text="User Profile" textFill="WHITE" />
      <Button fx:id="submitReportButton" layoutX="192.0" layoutY="146.0" mnemonicParsing="false" onAction="#openSubmitReportPage" styleClass="Button" stylesheets="@style.css" text="Submit Report" textFill="WHITE" />
      <Button fx:id="submitQualityReport" layoutX="135.0" layoutY="193.0" mnemonicParsing="false" onAction="#openSubmitQualityReportPage" prefHeight="27.0" prefWidth="215.0" styleClass="Button" stylesheets="@style.css" text="Submit Quality Report" textFill="WHITE" />
      <Button fx:id="viewMap" layoutX="204.0" layoutY="107.0" mnemonicParsing="false" onAction="#openMap" styleClass="Button" stylesheets="@style.css" text="View Map" textFill="WHITE" />
      <Button fx:id="viewReportsButton" layoutX="193.0" layoutY="237.0" mnemonicParsing="false" onAction="#handleViewSubmittedReports" styleClass="Button" text="View Reports" textFill="WHITE" />
   </children>
</AnchorPane>
